<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.TrackWorkoutActivity">

    <TextView
            android:id="@+id/routineLabel4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Track Workout"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/routineLabel5" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="33dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/routineLabel7"
        android:layout_width="228dp"
        android:layout_height="23dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="183dp"
        android:layout_marginBottom="546dp"
        android:gravity="center"
        android:text="Bench Press (Barbell)"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8" />

    <TextView
            android:id="@+id/routineLabel5"
            android:layout_width="440dp"
            android:layout_height="48dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="19dp"
            android:gravity="center"
            android:text="Duration     Volume      Sets"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName8"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.259"/>
    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="427dp"
        android:layout_height="39dp"
        android:layout_marginBottom="7dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="     1s            0lb           0"
        app:layout_constraintBottom_toTopOf="@+id/routineLabel7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/routineLabel5" />
</androidx.constraintlayout.widget.ConstraintLayout>